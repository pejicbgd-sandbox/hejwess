
# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /hejwess/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /hejwess/index.php [L]
RewriteRule ^(.*)index\.(php|html?)$ /$1 [R=301,NC,L]
</IfModule>

# END WordPress
